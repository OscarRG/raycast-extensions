{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "md5",
  "title": "MD5",
  "description": "Turn strings into secret codes with a sprinkle of MD5 magic! This Raycast extension lets you create MD5 hashes in the blink of an eye.",
  "icon": "command-icon.png",
  "author": "oscarrg",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "String 2 MD5 Hash",
      "description": "Generate MD5 hash from string",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
